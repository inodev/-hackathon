
<div class="container"
     style="background-image: url('AR_TOP/top_titlebg.png');margin-top: 40px;">
	<div class="row">
		<img src="AR_top/top_title.png"
		     style="margin: auto; display: inherit; padding-top: 20px;">
 	</div>
	<div class="row" style="padding-top: 20px;">
		<div class="col-sm-2"></div>
		<div class="col-sm-8">
		  	<form class="form-horizontal">
		  		<div class="form-group">
		  			<label for="name" class="col-sm-2 control-label"
		  			       style="color: white;">
		  				Name
	  				</label>
	  				<div class="col-sm-10">
	  					<input type="text" class="form-control"
	  						   id="name" placeholder="Your name">
	  				</div>
		  		</div>
		  		<div class="form-group">
		  			<label for="age" class="col-sm-2 control-label"
		  			       style="color: white;">
		  				Age
	  				</label>
	  				<div class="col-sm-10">
	  					<select class="form-control">
	  						<option value="0"> Unknown </option>
	  						<option value="1"  > 10 - 20 </option>
	  						<option value="2"  > 20 - 30 </option>
	  						<option value="3"  > 30 - 50 </option>
	  					</select>
	  				</div>
		  		</div>
		  		<div class="form-group">
		  			<label for="age" class="col-sm-2 control-label"
		  			       style="color: white;">
		  				Scale
	  				</label>
	  				<div class="col-sm-10">
	  					<select class="form-control">
	  						<option value="local"> Local scale </option>
	  						<option value="country"> Country scale </option>
	  						<option value="cosmic"> Cosmic scale </option>
	  					</select>
	  				</div>
		  		</div>
		  		<button type="submit"
		  		        style="padding: 0px; margin: auto; margin-top: 20px;
		  		         margin-left: 70px;">
		  		    <img src='AR_top/top_btn.png'>
		  		</button>
		  	</form>
		  	</div>
			<div class="col-sm-2"></div>
		</div>
	</div>
</div>

<script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>